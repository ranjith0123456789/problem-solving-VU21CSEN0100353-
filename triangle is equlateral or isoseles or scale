#include<stdio.h>
int main()
{
	int a,b,c;
    printf("enter the values of a,b,c ");
	scanf("%d%d%d",&a,&b,&c);
	if(a==b&&b==c)
	{
		printf("the riangle is equilateral");
	 } 
	 else  if(a==b||b==c||c==a)
	 {
	 	printf("the triangle is isoseles");	
	 }
	 else 
	 {
	 	printf("the triangle is scalen");
	 }
	return 0;
    }
