#include<stdio.h>
int main()
{
	int i,j,row,col,a[10][10],b[10][10],c[10][10];
	printf("to performe a addition of matrix");
	scanf("%d",&row);
	scanf("%d",&col);
	for(i=0;i<=row;i++)
	{
		for(j=0;j<=col;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=0;i<=row;i++)
	{
		for(j=0;j<=col;j++)
		{
			scanf("%d",&b[i][j]);
			c[i][j]=a[i][j]+b[i][j];
			
		}
	}
	for(i=0;i<=row;i++)
	{
		for(j=0;j<=col;j++)
		{
			printf("%d",c[i][j]);
		}
		printf("\n");

	return 0;
}




