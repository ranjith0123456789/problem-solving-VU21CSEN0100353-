#include<stdio.h>
int main()
{
	int a,b,c;
	printf("enter the values of a,b,c");
	scanf("%d%d%d",&a,&b,&c);
	if (a<b&&a<c)
	{
		printf("the first number is minimum");
	}
	else if (b<c&&b<a)
	{
		printf("the second number is minimum");
	}
	else if(c<a&&c<b)
	{
		printf("the third number is minimum");
	}
	return 0;
}
