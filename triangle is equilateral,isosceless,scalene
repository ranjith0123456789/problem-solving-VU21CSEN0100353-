#include<stdio.h>
int main()
{
	int a,b,c;
	printf("enter the value of a,b,c");
	scanf("%d%d%d",&a,&b,&c);
	if(a==b&&b==c)
	{
		printf(" the triangle is equilateral");
	}
		else if (a==b||b==c||c==a)
	{
		printf("the triangle is isosceless");
	}
	else
	{
		printf("the triangle is scalene");
	}
	return 0;    
	}
